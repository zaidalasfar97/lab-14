<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/style/layout.css">
</head>
<body>
    <%- include('../../layout/header.ejs') %>
    <main>

    <% Data.forEach(element => { %>
  <div>
 <img src="<%=element.image_url%>" alt="">
        <p>  <%= element.title%></p>
        <p>  <%= element.author%></p>
        <p>  <%= element.description%></p>
     <form action='/books' method="POST" type="hidden">
        <!-- <input type="text" id="custId" name="custId" value="3487"> -->
    <input type="hidden" name="title" value="<%=element.title  %> ">
    <input type="hidden" name="author" value="<%=element.author  %> ">
    <input type="hidden" name="description" value="<%=element.description  %> ">
    <input type="hidden" name="image_url" value="<%=element.image_url  %> ">
    <input type="hidden" name="isbn" value="<%=element.isbn  %> ">
<button>save</button>
    </form>
    </div>
        <!-- this.imageURL=data.volumeInfo.imageLinks.thumbnail || "https://i.imgur.com/J5LVHEL.jpg"
        this.bookTitle=data.volumeInfo.title|| "There Is No Title"
        this.authorName=data.volumeInfo.authors || "There Is No Author"
        this.description=data.volumeInfo.description || "There Is No Description"
         -->
    <% }) %>
</main>
<%- include('../../layout/footer.ejs') %>
</body>
</html>